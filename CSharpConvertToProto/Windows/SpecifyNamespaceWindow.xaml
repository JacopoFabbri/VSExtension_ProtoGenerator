<Window x:Class="CSharpConvertToProto.Windows.SpecifyNamespaceWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Select Enum Components" Height="200" Width="300">
    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Width="248">
            <Label Content="Specifica Namespace" FontSize="16" Margin="10"/>
            <TextBox Name="NamespaceTextBox" Width="150" Height="25" Margin="10"/>
            <Button Content="Submit" Width="100" Height="30" Margin="10" Click="Submit_Click"/>
        </StackPanel>
    </Grid>
</Window>