<Window x:Class="CSharpConvertToProto.ClassSelectionWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Seleziona Classe" Height="250" Width="400">
    <Grid>
        <ListBox Name="ClassList" Margin="10" SelectionMode="Single"/>
        <Button Content="Seleziona" HorizontalAlignment="Right" Margin="10" VerticalAlignment="Bottom" Width="80"
                Click="OnSelectClick"/>
    </Grid>
</Window>
